{
  "name": "shopify-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:noindex": "nuxt generate && echo 'User-agent: *\nDisallow: /' > dist/robots.txt",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
