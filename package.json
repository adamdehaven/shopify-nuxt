    "url": "https://github.com/adamdehaven/shopify-nuxt.git"
  },
  "author": "Adam DeHaven <adam@dehaven.org> (https://www.adamdehaven.com/)",
  "license": "UNLICENSED",
  "homepage": "https://github.com/adamdehaven/shopify-nuxt#readme",
  "dependencies": {
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^3.8.3",
    "lodash.debounce": "^4.0.8",
    "nuxt": "^2.14.12",
    "nuxt-shopify": "^1.10.7",
    "prism-themes": "^1.6.0",
    "vue-awesome": "^4.1.0",
    "vue-inline-svg": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^1.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "4",
    "prettier": "^2.2.1",
